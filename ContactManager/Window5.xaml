<Window x:Class="ContactManager.Window5"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ContactManager"
        mc:Ignorable="d"
        Title="contact info" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <Label Content="First Name:"/>
            <TextBox Name="viewFN" IsReadOnly="True" Margin="5,5,5,5"/>

            <Label Content="Last Name:"/>
            <TextBox Name="viewLN" IsReadOnly="True" Margin="5,5,5,5"/>

            <Label Content="Phone:"/>
            <TextBox Name="viewPN" IsReadOnly="True" Margin="5,5,5,5"/>

            <Label Content="Email:"/>
            <TextBox Name="viewE" IsReadOnly="True" Margin="5,5,5,5"/>
        </StackPanel>
    </Grid>
</Window>
